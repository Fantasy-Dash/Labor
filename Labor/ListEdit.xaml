<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labor"
        xmlns:Collections="clr-namespace:System.Collections;assembly=mscorlib" x:Class="Labor.ListEdit"
        mc:Ignorable="d"
        Title="ListEdit" Height="450" Width="339" Closing="Window_Closing">
    <Grid>
        <ListBox x:Name="ListBox_Data" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,10,95,211"/>
        <TextBox x:Name="TextBox_Input" HorizontalAlignment="Left" Margin="93,288,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Button_Delete" Content="删除" HorizontalAlignment="Left" Margin="256,52,0,0" VerticalAlignment="Top" Height="41" Width="63" Click="Button_Delete_Click"/>
        <Button x:Name="Button_Add" HorizontalAlignment="Left" Margin="263,189,0,0" VerticalAlignment="Top" Content="新增" Height="35" Width="49" Click="Button_Add_Click"/>

    </Grid>
</Window>
